<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<TreeTableView xmlns:fx="http://javafx.com/fxml/1" fx:id="replayVaultRoot" prefHeight="200.0" prefWidth="200.0"
               showRoot="false" xmlns="http://javafx.com/javafx/8.0.40"
               fx:controller="com.faforever.client.vault.replay.ReplayVaultController">
  <columns>
      <TreeTableColumn fx:id="mapColumn" prefWidth="75.0" text="%game.map"/>
      <TreeTableColumn fx:id="titleColumn" prefWidth="75.0" text="%game.title"/>
      <TreeTableColumn fx:id="playersColumn" prefWidth="75.0" text="%game.players"/>
      <TreeTableColumn fx:id="timeColumn" prefWidth="75.0" text="%game.time"/>
      <TreeTableColumn fx:id="durationColumn" prefWidth="75.0" text="%game.duration"/>
      <TreeTableColumn fx:id="gameTypeColumn" prefWidth="75.0" text="%game.gameType"/>
      <TreeTableColumn fx:id="idColumn" prefWidth="75.0" text="%game.id"/>
  </columns>
  <columnResizePolicy>
    <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
  </columnResizePolicy>
</TreeTableView>
